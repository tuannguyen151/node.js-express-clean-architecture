{
  "name": "node-app",
  "version": "1.0.0",
  "main": "src/app.js",
  "author": "TSN",
  "description": "",
  "type": "module",
  "engines": {
    "node": "^16.15.1"
  },
  "scripts": {
    "start": "node --experimental-modules --es-module-specifier-resolution=node bin/www",
    "dev-docker": "../node_modules/nodemon/bin/nodemon.js --experimental-modules --es-module-specifier-resolution=node --debug=5858",
    "docker-migare:up": "../node_modules/.bin/sequelize-cli db:migrate",
    "docker-migare:undo": "../node_modules/.bin/sequelize-cli db:migrate:undo",
    "docker-migration:generate": "../node_modules/.bin/sequelize-cli migration:generate --name $*",
    "docker-seed:generate": "../node_modules/.bin/sequelize-cli seed:generate --name $*",
    "docker-seed:all": "../node_modules/.bin/sequelize-cli db:seed:all",
    "docker-seed:undo": "../node_modules/.bin/sequelize-cli db:seed:undo",
    "dev-host": "nodemon --debug=5858",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier": "prettier --ignore-path .gitignore . --check",
    "prettier:fix": "prettier --ignore-path .gitignore . --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "start-watch": "nodemon src/app.js --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon src/app.js --inspect-brk=0.0.0.0:9229",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.1",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1"
  }
}
